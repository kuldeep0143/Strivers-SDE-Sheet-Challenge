class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int maxOne=0;
        int ch=0;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]==1)
            {
                ch++;
                maxOne=Math.max(maxOne,ch);
                
               
            }
            else if(nums[i]==0)
            {
                ch=0;
            }
        }
        return maxOne;
        
    }
}
